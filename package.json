{
  "name": "notifique-me-admin",
  "version": "1.2.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "npx tsx server/_core/index.ts",
    "dev:watch": "npx tsx watch --ignore \"client/**\" --ignore \"**/*.timestamp*\" server/_core/index.ts",
    "dev:client": "vite --config client/vite.config.ts",
    "dev:server": "npx cross-env NODE_ENV=development npx tsx server/_core/index.ts",
    "build:client": "vite build --config client/vite.config.ts",
    "build:server": "npx esbuild server/_core/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build": "npm run build:client && npm run build:server",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "npx tsc --noEmit",
    "format": "npx prettier --write .",
    "test": "npx vitest run",
    "db:push": "npx drizzle-kit generate && npx drizzle-kit migrate",
    "db:init": "npx tsx scripts/init-db.ts",
    "db:migrate": "npx tsx scripts/init-db.ts",
    "setup": "npm install && npm run db:init && npm run check",
    "clean": "rimraf dist node_modules/.vite",
    "worker": "npx tsx scripts/process-schedules.ts --loop"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.693.0",
    "@aws-sdk/s3-request-presigner": "^3.975.0",
